{
  "name": "ngrx-rayes",
  "version": "1.0.2",
  "description": "Manage ngrx process and provide reusable generic classes that contains base code for actions, effects, selectors and reducer about crud operation",
  "main": "./lib/src/index.ts",
  "types": "./lib/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "postVersion": "git push && git push --tags"
  },
  "keywords": [
    "Redux",
    "NgRX",
    "Dispatch",
    "Action",
    "Effect",
    "Reducer",
    "Angular",
    "Dispatching",
    "Store",
    "Cache",
    "Caching"
  ],
  "author": "Louis Mary MONJENGUE",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "jest": "^27.1.0",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@ngrx/effects": "^12.4.0",
    "@ngrx/store": "^12.4.0",
    "rxjs": "^7.3.0"
  }
}
